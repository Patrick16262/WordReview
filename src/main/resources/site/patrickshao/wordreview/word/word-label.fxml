<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="50.0" maxWidth="200.0" prefHeight="50.0" prefWidth="200.0" stylesheets="@word.css" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="site.patrickshao.wordreview.gui.controller.word.WordLabel">
   <children>
      <HBox fx:id="label_pane" alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" styleClass="selectable-pane">
         <children>
            <Label fx:id="content" styleClass="selectable-label" text="word" />
            <HBox alignment="CENTER_RIGHT" HBox.hgrow="ALWAYS">
               <children>
                  <Label styleClass="selectable-label" text="&gt;" />
               </children>
            </HBox>
         </children>
         <VBox.margin>
            <Insets left="10.0" right="10.0" />
         </VBox.margin>
      </HBox>
      <Separator prefWidth="200.0" scaleY="0.5" />
   </children>
</VBox>
